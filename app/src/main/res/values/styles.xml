<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

    <style name="Circular">
        <item name="cornerSize">50%</item>
    </style>

    <style name="AppToolbar" parent="@style/ThemeOverlay.AppCompat.ActionBar">
        <item name="android:textColorPrimary">@color/GreyWhite</item>
        <item name="android:textColorSecondary">@color/GreyWhite</item>
    </style>

    <style name="ActionBar" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

</resources>
